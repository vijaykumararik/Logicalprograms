package patern1;

import java.util.Scanner;

public class proin {

	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.println("enter value");
		int n=s.nextInt();
		int c=0,x=0;
		for(int i=1;i<=100;i++)
		{
			if(n%i==0)
			{
				c++;
				
				}
			if(c==2)
			{   x++;
				if(n==x)
				{
					System.out.println(i);
					break;
				}
			}
			
		}
		
	}

}
