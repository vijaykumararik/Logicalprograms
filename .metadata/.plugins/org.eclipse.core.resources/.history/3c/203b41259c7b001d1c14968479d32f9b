package patern1;

import java.util.Scanner;

public class fibno {
public static void main(String[] args) {
	Scanner s=new Scanner(System.in);
	System.out.println("enter value to print");
	int n=s.nextInt();
	int sum=0,x=0,y=1;
	while(sum<=8)
	{
		System.out.println(sum);
		x=y;
		y=sum;
		sum=x+y;
		
		
	}
}
}
